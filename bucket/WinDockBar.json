        "backup",
        "launchpad",
        "config.ini",
        "ico.ini",
        "padconfig.xml"
    ],

    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/AUGUSTDzw/WinDockBar/releases/download/WinDockBar$version/WinDockBar$version.zip"
            }
        },
        "hash": {
